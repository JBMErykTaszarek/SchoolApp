@model IEnumerable<SchoolApp.Models.Note>

@{
    ViewBag.Title = "CheckKidNotes";
}

<h2>All Notes had been given to your Child</h2>


<table class="table">
    <tr>
        <th>
            <p>Task</p>
        </th>
        <th>
            <p>Note</p>
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ForTask)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.WchitNote)
        </td>
       
        
    </tr>
}

</table>
