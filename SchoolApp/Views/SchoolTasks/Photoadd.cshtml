
@{
    ViewBag.Title = "Add Your Kid Photo";
}

<h2>Add Your Kid Photo</h2>

<div class="row">
    <div class="col-md-10">
        @using (Html.BeginForm("Photoadd","SchoolTasks", FormMethod.Post, new {@enctype = "multipart/form-data" }))
        {
        <input type="file" name="file"/>
        <br />
        <input type="submit" value="Upload" />
        }
    </div>
</div>
