@model IEnumerable<SchoolApp.Models.ApplicationUser>

@{
    ViewBag.Title = "GetMyStudents";
}

<h2>GetMyStudents</h2>



<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Email)
        </th>
    </tr>p

    @foreach (var item in Model)
    {
        <tr>
            <td>
                <a href="@Url.Action("GetStudentNotes", "SchoolTasks", new { email=item.Email})">@Html.DisplayFor(modelItem => item.Email)</a>
            </td>
           
        </tr>
    }

</table>
